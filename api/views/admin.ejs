<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Page</title>
</head>
<body>
    <h1>Admin Dashboard</h1>
    <h2>Users List</h2>
    <ul>
        <% users.forEach(user => { %>
            <li><%= user.email %> <%= user.id %></li>
        <% }) %>
    </ul>
    <form action="/logout" method="POST">
        <button type="submit">Logout</button>
    </form>
</body>
</html>